#!env pscript
import("service");

insmod("pssm");
insmod("mem_pipe");
insmod("tcp_pipe 12345");
runtime.servlet.path="./bin"

graph = {
	read  := "readsock";
	first := "first 3";
	write := "writesock";

	() -> "in" read {
		"out0" -> "in0";
		"out1" -> "in1";
		"out2" -> "in2";
	} first "out" -> "in" write "out" -> ();
};

var info = Service.typeinfo(graph);
for(var key in info)
	print(key, ":", info[key]);

Service.start(graph);
